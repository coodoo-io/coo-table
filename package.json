{
  "name": "coo-table",
  "version": "1.0.0",
  "description": "The coodoo table angular 2 component",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "private": true,
  "scripts": {
    "test": "karma start --watch false",
    "test:watch": "karma start ---watch true --single-run false",
    "test:coverage": "COVERAGE=true npm test",
    "start:app": "cd demo; npm start",
    "build:ngc": "npm run clean; npm run inline; ngc -p tsconfig-ngc.json; rimraf .dist; copyfiles README.MD HISTORY.MD ./dist; cp _package.json ./dist/package.json",
    "build:demo": "npm run clean && cd demo && ng build --prod --aot=false --output-hashing none",
    "replace:demo": "replace '<base href=\"/\">' '<base href=\"**REPLACE**\">' ./demo/dist/src/index.html && replace 'src=\"../' 'src=\"./../' ./demo/dist/src/index.html && replace 'href=\"/src/' 'href=\"./' ./demo/dist/src/index.html",
    "ts-lint": "tslint -c tslint.json './src/**/*.ts' -e './src/**/*.spec.ts' -e ./src/test.ts || true",
    "inline": "ng2inline --outDir=./.dist './src/**/*.ts'",
    "inline:demo": "ng2inline --outDir=./.dist/demo './demo/src/**/*.ts' && ng2inline --outDir=./.dist/src './src/**/*.ts'",
    "clean": "rimraf dist .dist delivery coverage docs demo-dist demo/dist/",
    "postinstall": "cd demo; npm install",
    "compodoc": "compodoc -p tsconfig-compodoc.json -d docs --disableGraph --disableCoverage --disablePrivateOrInternalSupport -t",
    "publish:npm": "**insert your code here**",
    "publish:git": "**insert your code here**"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/coodoo-gmbh/coo-table"
  },
  "author": {
    "name": "coodoo GmbH",
    "email": "mail@coodoo.de",
    "url": "http://coodoo.io"
  },
  "keywords": [
    "ng2",
    "angular2",
    "table",
    "listing",
    "ngx",
    "jaxrs"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/coodoo-gmbh/coo-table/issues"
  },
  "homepage": "https://bitbucket.org/coodoo-gmbh/coo-table/readme",
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.0.8",
    "codelyzer": "~2.0.0",
    "compodoc": "0.0.41",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "intl": "^1.2.5",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-story-reporter": "^0.3.1",
    "ng2-inline-template": "^0.1.1",
    "protractor": "~5.1.0",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.1.0",
    "ts-node": "^2.0.0",
    "tslint": "~4.5.0",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.2.2",
    "webpack": "^2.2.0",
    "webpack-angular-externals": "^1.0.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-rxjs-externals": "^1.0.0",
    "zone.js": "^0.8.4"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "ionicons": "^3.0.0",
    "@angular/router": "^4.0.0"
  },
  "peerDependencies": {
    "@angular/core": "4.0.0",
    "@angular/common": "4.0.0"
  },
  "engines": {
    "node": ">=0.8.0"
  }
}